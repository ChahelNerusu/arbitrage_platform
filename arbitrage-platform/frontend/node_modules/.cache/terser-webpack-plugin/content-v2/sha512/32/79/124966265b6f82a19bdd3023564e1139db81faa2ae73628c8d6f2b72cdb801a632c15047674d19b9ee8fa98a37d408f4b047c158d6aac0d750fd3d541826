           <tr key={index}>\n                            <td className=\"py-2 px-4 border-b\">{opportunity.asset}</td>\n                            <td className=\"py-2 px-4 border-b\">{opportunity.exchangeA}</td>\n                            <td className=\"py-2 px-4 border-b\">{opportunity.exchangeB}</td>\n                            <td className=\"py-2 px-4 border-b\">{opportunity.spread}%</td>\n                            <td className=\"py-2 px-4 border-b\">${opportunity.estimatedProfit}</td>\n                            <td className=\"py-2 px-4 border-b\">{new Date(opportunity.lastUpdated).toLocaleString()}</td>\n                        </tr>\n                    ))}\n                </tbody>\n            </table>\n        </div>\n    );\n};\n\nexport default Table;","import React from 'react';\n\nconst Filters = ({ onFilterChange }) => {\n    const [assetType, setAssetType] = React.useState('');\n    const [region, setRegion] = React.useState('');\n    const [riskLevel, setRiskLevel] = React.useState('');\n    const [profitMargin, setProfitMargin] = React.useState('');\n\n    const handleFilterChange = () => {\n        onFilterChange({\n            assetType,\n            region,\n            riskLevel,\n            profitMargin,\n        });\n    };\n\n    return (\n        <div className=\"filters\">\n            <h2>Filter Opportunities</h2>\n            <div className=\"filter-group\">\n                <label>Asset Type:</label>\n                <select value={assetType} onChange={(e) => setAssetType(e.target.value)} onBlur={handleFilterChange}>\n                    <option value=\"\">All</option>\n                    <option value=\"crypto\">Crypto</option>\n                    <option value=\"stocks\">Stocks</option>\n                    <option value=\"commodities\">Commodities</option>\n                    <option value=\"forex\">Forex</option>\n                </select>\n            </div>\n            <div className=\"filter-group\">\n                <label>Region:</label>\n                <input type=\"text\" value={region} onChange={(e) => setRegion(e.target.value)} onBlur={handleFilterChange} />\n            </div>\n            <div className=\"filter-group\">\n                <label>Risk Level:</label>\n                <select value={riskLevel} onChange={(e) => setRiskLevel(e.target.value)} onBlur={handleFilterChange}>\n                    <option value=\"\">All</option>\n                    <option value=\"low\">Low</option>\n                    <option value=\"medium\">Medium</option>\n                    <option value=\"high\">High</option>\n                </select>\n            </div>\n            <div className=\"filter-group\">\n                <label>Profit Margin (%):</label>\n                <input type=\"number\" value={profitMargin} onChange={(e) => setProfitMargin(e.target.value)} onBlur={handleFilterChange} />\n            </div>\n        </div>\n    );\n};\n\nexport default Filters;","import React, { useState, useEffect } from 'react';\n\nconst Alerts = ({ opportunities }) => {\n    const [alerts, setAlerts] = useState([]);\n\n    useEffect(() => {\n        const newAlerts = opportunities.filter(opportunity => opportunity.estimatedProfit > 0.05); // Example threshold\n        setAlerts(newAlerts);\n    }, [opportunities]);\n\n    return (\n        <div className=\"alerts\">\n            <h2 className=\"text-lg font-bold\">Alerts</h2>\n            {alerts.length > 0 ? (\n                <ul className=\"list-disc pl-5\">\n                    {alerts.map((alert, index) => (\n                        <li key={index} className=\"my-2\">\n                            <strong>{alert.asset}</strong>: Profit potential of {alert.estimatedProfit * 100}% between {alert.exchangeA} and {alert.exchangeB}.\n                        </li>\n                    ))}\n                </ul>\n            ) : (\n                <p>No alerts at this time.</p>\n            )}\n        </div>\n    );\n};\n\nexport default Alerts;","import React, { useEffect, useState } from 'react';\nimport axios from 'axios';\nimport Table from './Table';\nimport Filters from './Filters';\nimport Alerts from './Alerts';\n\nconst Dashboard = () => {\n    const [opportunities, setOpportunities] = useState([]);\n    const [loading, setLoading] = useState(true);\n    const [filters, setFilters] = useState({\n        assetType: '',\n        region: '',\n        riskLevel: '',\n        profitMargin: ''\n    });\n\n    useEffect(() => {\n        const fetchOpportunities = async () => {\n            try {\n                const response = await axios.get('/api/opportunities');\n                setOpportunities(response.data);\n            } catch (error) {\n                console.error('Error fetching opportunities:', error);\n            } finally {\n                setLoading(false);\n            }\n        };\n\n        fetchOpportunities();\n        const interval = setInterval(fetchOpportunities, 5000); // Update every 5 seconds\n\n        return () => clearInterval(interval);\n    }, []);\n\n    const handleFilterChange = (newFilters) => {\n        setFilters(newFilters);\n    };\n\n    const filteredOpportunities = opportunities.filter(opportunity => {\n        // Apply filtering logic based on the filters state\n        return (\n            (!filters.assetType || opportunity.asset === filters.assetType) &&\n            (!filters.region || opportunity.region === filters.region) &&\n            (!filters.riskLevel || opportunity.riskLevel === filters.riskLevel) &&\n            (!filters.profitMargin || opportunity.profitMargin >= filters.profitMargin)\n        );\n    });\n\n    return (\n        <div className=\"min-h-screen bg-[#181e29] text-white flex flex-col items-center py-10 dashboard\">\n            <h1 className=\"text-4xl font-bold mb-2\">Arbitrage Opportunities</h1>\n            <p className=\"text-lg text-[#a0aec0] mb-8\">Arbitrage Opportunities Across the World</p>\n            <div className=\"bg-[#232b3e] rounded-xl shadow-lg p-8 w-full max-w-4xl\">\n                <Filters onFilterChange={handleFilterChange} />\n                {loading ? (\n                    <p className=\"text-[#a0aec0]\">Loading opportunities...</p>\n                ) : (\n                    <Table opportunities={filteredOpportunities} />\n                )}\n                <Alerts opportunities={filteredOpportunities} />\n            </div>\n        </div>\n    );\n};\n\nexport default Dashboard;","import React, { useState, useEffect } from 'react';\nimport Dashboard from './components/Dashboard';\nimport Filters from './components/Filters';\nimport Alerts from './components/Alerts';\nimport Table from './components/Table';\n\nconst App = () => {\n    const [opportunities, setOpportunities] = useState([]);\n    const [filters, setFilters] = useState({\n        assetType: '',\n        region: '',\n        riskLevel: '',\n        profitMargin: ''\n    });\n\n    useEffect(() => {\n        const fetchOpportunities = async () => {\n            const response = await fetch('/api/opportunities');\n            const data = await response.json();\n            setOpportunities(data);\n        };\n\n        fetchOpportunities();\n    }, []);\n\n    const handleFilterChange = (newFilters) => {\n        setFilters(newFilters);\n    };\n\n    return (\n        <div className=\"app\">\n            <h1>Arbitrage Opportunities</h1>\n            <Filters onFilterChange={handleFilterChange} />\n            <Alerts opportunities={opportunities} />\n            <Dashboard opportunities={opportunities} />\n            <Table opportunities={opportunities} filters={filters} />\n        </div>\n    );\n};\n\nexport default App;","import React from 'react';\nimport ReactDOM from 'react-dom';\nimport App from './App';\nimport './styles/tailwind.css';\n\nReactDOM.render(\n  <React.StrictMode>\n    <App />\n  </React.StrictMode>,\n  document.getElementById('root')\n);"]}}